#include<stdio.h>
int main()
{
	int a = 10;
	char arr[10] = { 0 };
	int arr2[10] = { 0 };
	short int b = 10;

	printf("%d\n", sizeof(b));
	printf("%d\n", sizeof (a) );

	printf("%d\n", sizeof (int[10]) );
	
	printf("%d\n", sizeof a);

	printf("%d\n", sizeof (arr) );

	printf("%d\n", sizeof (arr2) );
	printf("%d\n", sizeof (int [10]));

	


}
