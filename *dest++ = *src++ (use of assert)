#include<stdio.h>
#include<string.h>
#include<assert.h>
char* my_strcpy(char* dest, const char* src)
{
	assert(src != NULL);
	assert(dest != NULL);
	char* ret = dest;
	while (*dest++ = *src++)
	{
		;
	}
	return ret;
}

int main()
{
	char arr1[20] = "xxxxxxxxx";
	char arr2[] = "hello";

	printf("%s\n", my_strcpy(arr1, arr2));
	printf("%s\n", arr2);
	return 0;
}
