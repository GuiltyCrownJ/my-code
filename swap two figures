# my-code\
#include<stdio.h>
#define _CRT_SECURE_NO_WARNINGS 
#include <string.h>
void swap(int *x, int *y)
{
    int z = 0;
    z = *x;
    *x = *y;
    *y = z;

}
int main()
{
    int a = 10;
    int b = 20;
    int *pa = &a;
    printf("交换前 %d %d", a,b);
    swap(&a, &b);
    printf("交换后 %d %d", a,b);

}
