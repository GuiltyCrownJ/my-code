#include<stdio.h>
#include<string.h>
#include<assert.h>

int my_strlen(const char* arr)
{
	assert(arr != NULL);
	int count = 0;
	while (*arr++)
	{
		count++;

	}
	return count;
}

int main()
{
	char arr1[20] = "xxxxxxxxx";
	char arr2[] = "hello";

	printf("%d\n", my_strlen(arr1));
	printf("%d\n", my_strlen(arr2));
	return 0;
}
