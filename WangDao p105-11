# my-code
semaphore s1 = 0;
semaphore s2 = 0;
semaphore s3 = 0;
semaphore s4 = 0;
p1()
{
	while (1)
	{

		input a;
		v(s1);

		p(sb);
		x = a + b;

		p(s4);
		print x, y, z;

	}
}

p2()
{
	while (1)
	{

		p(s1);
		input b;
		v(sb);
		v(s2);
		y = a * b;		
		v(s3);
	}
}

p3()
{
	while (1)
	{
		p(s2);
		input c;

		p(s3);
		z = y + c - a;
		v(s4);

	}
}
