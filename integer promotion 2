#include<stdio.h>

int main()
{
    char a = 0xb6;
    //10110110
    //111111111111111111111111 10110110
    short b = 0xb600;
    //10110110 00000000
    //1111111111111111 10110110 00000000
    int c = 0xb6000000;
    //10110110 00000000 00000000 00000000

    //100000000000000000000000 01001001
    //a=0x80000049
    if (a == 0xb6)
        printf("a");
    //1000000000000000 01001001 11111111
    //b=0x800049FF
    if (b == 0xb600)
        printf("b");
    //10110110 00000000 00000000 00000000
    //c=0xb6000000
    if (c == 0xb6000000)
        printf("c");


    return 0;
}
