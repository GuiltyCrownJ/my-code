#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
	char a = -128;
	//10000000000000000000000010000000
	//11111111111111111111111101111111
	//11111111111111111111111110000000
	//                      a=10000000
	printf("%u", a);
//%u  11111111111111111111111110000000   补码 （整型提升补符号位1）
//%u  (unsigned int 打印)   a=4294967168
    

    // int = signed int; long and short 类似


	return 0;
}
